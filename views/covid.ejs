<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="https://unpkg.com/element-ui/lib/theme-chalk/index.css">
    <title>Covid</title>
</head>
<body>
    <h1 id="stats" >{{contenu}}</h1>
    <div id="Statistique">
        <el-container style="height:500px;border:1px solid #eee">
        <el-container id="table">
            <el-table :data="tableData">
                <el-table-column prop="cases" label="world case" width="140"></el-table-column>
                <el-table-column prop ="france"label="france"width="140"></el-table-column>
                <el-table-column prop ="today"label="today cases france"width="140"></el-table-column>
                <el-table-column prop ="update"label="updateDate" width="140"></el-table-column>
            </el-table>
            <!--<p> <%=data.cases%></p>
            <p> <%=france.cases%></p>
            <p><%=france.todayCases%></p>
            <p id="updateDate">{{msg}}</p>
            !-->
        </el-container>
    </el-container>
    </div>
    <script src="https://cdn.jsdelivr.net/npm/vue@2/dist/vue.js"></script>
    <script src="https://unpkg.com/element-ui/lib/index.js"></script>
    <script>
        var content ={
            contenu:"Statistiques sur le covid-19"
        }
        var msg={
            msg:"date des donn√©es:<%=france.updatedDate%>"
        }
        var app = new Vue({
            el:"#stats",
            data:content
        })
        var locale;
        var date
       var date = new Vue({
           data:{a:'<%=france.updatedDate%>'},
           methods:{
               plus:function(){
                    date =new Date(this.a);
                    locale=date.toLocaleString();
               }
           }
       })
       date.plus();
       
       
        var table =new Vue({
            el:"#table",
            data(){
                const items={
                    cases:'<%=data.cases%>',
                    france:'<%=france.cases%>',
                    today:'<%=france.todayCases%>',
                    update:locale
                }
                return{
                    tableData:Array(1).fill(items)
                }
            }
        })
    </script>
</body>
</html>